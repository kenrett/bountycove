= form_for [current_user, treasure] do |f|
  = f.label :name
  = f.text_field :name, :required => true
  %br
  = f.label :description
  = f.text_field :description, :required => true
  %br
  = f.label :photo
  = f.text_field :photo
  %br
  = f.label :price
  = f.text_field :price, :required => true
  %br
  = f.submit "Create Treasure"
